#: panel-plugin/main.c:188
#, c-format
msgid "%s: Device statistics unavailable."
msgstr "%s: Statistika naprave ni na voljo."

#: panel-plugin/main.c:227
#, c-format
msgid ""
"%s\n"
"----------------\n"
"I/O (MiB/s)\n"
"  Read: %3.2f\n"
"  Write: %3.2f\n"
"  Total: %3.2f\n"
"Busy time (%c)\n"
"  Read: %d\n"
"  Write: %d\n"
"  Total: %d"
msgstr "%s\n----------------\nVhod/izhod (MiB/s)\n  Branje:%3.2f\n  Pisanje:%3.2f\n  Skupaj:%3.2f\nZasedenost (%c)\n  Branje: %d\n  Pisanje: %d\n  Skupaj: %d"

#: panel-plugin/main.c:823
#, c-format
msgid ""
"%s\n"
"%s: %s (%d)\n"
"\n"
"This monitor will not work!\n"
"Please remove it."
msgstr "%s\n%s: %s (%d)\n\nTa prikaz ne bo deloval\nOdstranite ga, prosim."

#: panel-plugin/main.c:834
#, c-format
msgid ""
"%s: No disk extended statistics found!\n"
"Either old kernel (< 2.4.20) or not\n"
"compiled with CONFIG_BLK_STATS turned on.\n"
"\n"
"This monitor will not work!\n"
"Please remove it."
msgstr "%s: Ni razširjene statistike!\nLahko gre za staro jedro (< 2.4.20) ali ni bil\npreveden z vklopljenim CONFIG_BLK_STATS.\n\nTa prikaz ne bo deloval!\nOdstranite ga, prosim."

#: panel-plugin/main.c:842
#, c-format
msgid ""
"%s: Unknown error\n"
"\n"
"This monitor will not work!\n"
"Please remove it."
msgstr "%s: Neznana napaka\n\nTa prikaz ne bo deloval!\nOdstranite ga, prosim."

#: panel-plugin/main.c:866
msgid ""
"Diskperf monitor displays instantaneous disk I/O transfer rates and busy "
"times"
msgstr "Diskperf prikazovalnik, prikaže trenutni vhodno-izhodni pretok diska in čas zasedenosti"

